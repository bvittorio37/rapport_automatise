{% extends 'base.html.twig' %}

{% block title %}TypeStock index{% endblock %}

{% block body %}
    <h1>TypeStock index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>TypeStock</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_stock in type_stocks %}
            <tr>
                <td>{{ type_stock.id }}</td>
                <td>{{ type_stock.typeStock }}</td>
                <td>
                    <a href="{{ path('app_type_stock_show', {'id': type_stock.id}) }}">show</a>
                    <a href="{{ path('app_type_stock_edit', {'id': type_stock.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_type_stock_new') }}">Create new</a>
{% endblock %}
